{
  "containerDefinitions": [
    {
      "essential": true,
      "name": "survey_site",
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp"
        }
      ]
    }
  ],
  "cpu": "512",
  "executionRoleArn": "arn:aws:iam::452410152928:role/ecsTaskExecutionRole",
  "family": "fargate-task-definition",
  "memory": "1024",
  "networkMode": "awsvpc",
  "runtimePlatform": {
    "operatingSystemFamily": "LINUX"
  },
  "requiresCompatibilities": [
    "FARGATE"
  ]
}
